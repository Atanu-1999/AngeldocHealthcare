                                android:layout_height="wrap_content"
                                android:layout_margin="5dp"
                                android:text="Health Records"
                                android:textColor="#626262"
                                android:textSize="@dimen/_13sdp"
                                android:textStyle="bold" />

                        </LinearLayout>

                        <LinearLayout
