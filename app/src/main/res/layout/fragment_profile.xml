                                android:layout_height="@dimen/_25sdp"
                                android:layout_gravity="center"
                                android:layout_marginLeft="@dimen/_10sdp"
                                android:padding="@dimen/_3sdp"
                                android:src="@drawable/arrow_forward_ios_24" />

                        </LinearLayout>

                    </LinearLayout>

                    <LinearLayout
